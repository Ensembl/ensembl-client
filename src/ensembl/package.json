{
  "name": "ensembl-new",
  "version": "0.0.1",
  "description": "New website of Ensembl",
  "main": "index.js",
  "directories": {
    "example": "example"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "EMBL-EBI",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve:dev",
    "serve:dev": "webpack-dev-server --config ./webpack/webpack.config.dev.js --hot --inline",
    "serve:prod": "node ./server.js --NODE_ENV=production",
    "build": "rimraf ./dist && webpack --config ./webpack/webpack.config.prod.js",
    "deploy": "node deploy",
    "certify": "node setup-ssl",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "tslint -c tslint.json 'src/**/*.{ts,tsx}' -t codeFrame --force",
    "lint:styles": "stylelint 'src/styles/**/*.scss'",
    "jest": "jest",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "snapshot": "jest --updateSnapshot"
  },
  "lint-staged": {
    "*.{scss,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "foundation-sites": "^6.5.3",
