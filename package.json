    "lodash": "4.17.23",
    "morgan": "1.10.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-redux": "9.2.0",
    "react-router": "7.13.0",
    "react-router-dom": "7.13.0",
    "redux": "5.0.1",
    "redux-observable": "3.0.0-rc.3",
    "redux-thunk": "3.1.0",
    "rxjs": "7.8.2"
