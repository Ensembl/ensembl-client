    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-redux": "9.2.0",
    "react-router": "7.9.2",
    "react-router-dom": "7.9.2",
    "redux": "5.0.1",
    "redux-observable": "3.0.0-rc.2",
    "redux-thunk": "3.1.0",
    "rxjs": "7.8.2"
